						<field>
							<name>AA</name>
							<description>Assert acknowledge flag.</description>
							<bitRange>[2:2]</bitRange>
						</field>
						<field>
							<name>SI</name>
							<description>I2C interrupt flag.</description>
							<bitRange>[3:3]</bitRange>
